{
  "name": "ai-comparison-system",
  "version": "0.1.0",
  "description": "CLI工具，支持向多个AI平台（豆包、Kimi、DeepSeek等）同时提问并对比结果",
  "main": "build/index.js",
  "type": "module",
  "bin": {
    "ai-compare": "./build/cli/index.js"
  },
  "scripts": {
    "dev": "tsx src/cli/index.ts",
    "build": "tsc && npm run docs",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" && eslint --fix \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "check-format": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\" && eslint \"src/**/*.ts\"",
    "check-unused": "ts-prune",
    "check-imports": "depcheck",
    "check-todos": "grep -r '@deprecated' src/ || echo 'No deprecated code found'",
    "docs": "echo 'Documentation generation placeholder'",
    "clean": "rm -rf build coverage .nyc_output",
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('Please use npm install')\"",
    "postinstall": "echo 'Dependencies installed successfully'"
  },
  "keywords": [
    "ai",
    "comparison",
    "cli",
    "doubao",
    "kimi",
    "deepseek",
    "automation",
    "browser-automation",
    "puppeteer"
  ],
  "author": "situruimin",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "chrome-devtools-mcp": "latest",
    "puppeteer-core": "^22.0.0",
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "cli-table3": "^0.6.3",
    "better-sqlite3": "^9.2.2",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/better-sqlite3": "^7.6.8",
    "@types/inquirer": "^9.0.7",
    "@types/cli-table3": "^0.6.3",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "prettier": "^3.1.1",
    "ts-prune": "^0.10.3",
    "depcheck": "^1.4.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/situruimin/ai-comparison-system.git"
  },
  "bugs": {
    "url": "https://github.com/situruimin/ai-comparison-system/issues"
  },
  "homepage": "https://github.com/situruimin/ai-comparison-system#readme"
}
